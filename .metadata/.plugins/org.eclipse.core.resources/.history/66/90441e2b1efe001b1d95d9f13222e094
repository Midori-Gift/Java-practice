
public class lf01 {
	public static void main(String[] args) {
		int  number = 7;
		if (number < 5) {
			System.out.println(number + "は5歳未満です");
		}
		if (number < 10) {
			System.out.printl(number + "は10歳未満です");
		}
		
		if (number < 15) {
			System.out.println(number + "15歳未満です");
		}
	}

}
